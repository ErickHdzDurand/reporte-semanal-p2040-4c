{
  "version": 2,
  "builds": [
    {
      "src": "index.htm",
      "use": "@vercel/static"
    },
    {
      "src": "public/datos.csv",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.htm"
    },
    {
      "src": "/public/(.*)",
      "dest": "/public/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.htm"
    }
  ]
}